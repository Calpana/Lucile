{
  "version": "1.0.0-*",
  "title": "Lucile Framework Primitives Library",
  "copyright": "copyright Â© codeworx gmbh 2016",
  "description": "Core primitives for the Lucile Framework. For detailed Documentation visit http://www.github.com/codeworxOS/Lucile",
  "authors": [ "Raphael Schwarz" ],
  "packOptions": {
    "owners": [ "codeworx gmbh" ],
    "summary": "Core primitives for the Lucile Framework. For detailed Documentation visit http://www.github.com/codeworxOS/Lucile"
  },
  "dependencies": {
    "NETStandard.Library": "1.6.1",
    "Lucile.Shared": {
      "version": "1.0.0-*",
      "type": "build"
    },
    "StyleCop.Analyzers": {
      "version": "1.0.0",
      "type": "build"
    },
    "System.Dynamic.Runtime": "4.3.0",
    "System.Runtime.Serialization.Primitives": "4.3.0",
    "System.Collections.Immutable": "1.3.1",
    "System.Runtime.InteropServices": "4.3.0",
    "System.Linq.Queryable": "4.3.0",
    "Microsoft.Extensions.DependencyInjection.Abstractions": "1.1.0"
  },

  "frameworks": {
    "netstandard1.0": {
      "imports": "dnxcore50"
    }
  },
  "buildOptions": {
    "warningsAsErrors": true,
    "additionalArguments": [ "/ruleset:..\\..\\global\\Stylecop.ruleset", "/additionalfile:..\\..\\global\\stylecop.json" ]
  }
}